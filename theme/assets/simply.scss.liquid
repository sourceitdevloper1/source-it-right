 /* zipcode validation css start */
 .new_pincode-section{
  float: left;
  .new_pincode_div{
    margin-left: 5px;
    border: 1px solid #ddd;
    border-radius: 5px;
    padding-left: 10px;
    display: inline-block;
    margin-right: 5px;
    label#title{
      border-right: 1px solid #ddd;
      padding-right: 10px;
      margin-right: 10px;
    }
    .new_pincode_input{
      width: 200px;
      -webkit-appearance: initial;
      height: 40px;
      margin-left: 5px;
      outline: 0px;
    }
  }
  label#msg_error{
    color:#ff0000;
    padding-right: 20px;
    .available{
      color: #2d862d;
    }
  }

}
@media(max-width: 767px){
  label#msg_error{
   display: block; 
   padding: 5px;
 }
}
div[data-address-field="zip"]{
  display: none;
}
/* prepaid banner css start */
.section__content{
  .banner_image{
    margin-bottom: 20px;
    @media(max-width: 999px){
      margin-bottom: 10px;
    }
    img{
      width: 100%;
    }
  }
}
.hide{
  display: none !important;
}
/* prepaid banner css end */